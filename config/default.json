{
  "openapiConfig": {
    "filePath": "./bundledApi.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "storageExplorer": {
    "layerSourceDir": "/layerSources",
    "displayNameDir": "\\layerSources",
    "watchDirectory": "watch",
    "validFileExtensions": ["gpkg"]
  },
  "validationValuesByInfo": {
    "crs": [4326],
    "fileFormat": ["GPKG"],
    "pixelSizeRange": {
      "min": 1.67638e-7,
      "max": 0.703125
    },
    "tileSize": 256,
    "resolutionFixedPointTolerance": 12,
    "extentBufferInMeters": 50
  }
}
